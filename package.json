{
  "name": "webpack-es6-react-flux-example",
  "version": "0.0.0",
  "main": "server/main.js",
  "devDependencies": {
    "babel-core": "^5.8.8",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "concurrently": "^0.1.1",
