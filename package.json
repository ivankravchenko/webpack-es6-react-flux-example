{
  "name": "myacc-react",
  "version": "0.0.1",
  "main": "server/main.js",
  "devDependencies": {
    "assets-webpack-plugin": "^2.2.0",
    "babel-core": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "concurrently": "^0.1.1",
    "css-loader": "aluxian/css-loader",
    "eslint": "^1.0.0",
    "eslint-loader": "^0.14.2",
    "eslint-plugin-react": "^3.2.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "handlebars": "^3.0.3",
    "handlebars-loader": "^1.1.0",
    "jsdom": "3.1.2",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.2.5",
    "mockery": "^1.4.0",
    "path": "^0.11.14",
    "react-hot-loader": "^1.2.8",
    "source-map-support": "^0.3.2",
    "style-loader": "^0.12.3",
    "webpack": "^1.11.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.10.1",
    "webpack-error-notification": "^0.1.4"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint . --ext .js,.jsx; exit 0",
    "lint-watch": "nodemon --watch client --exec 'npm run lint'",
    "test": "./node_modules/.bin/mocha --recursive -c",
    "watch-test": "./node_modules/.bin/mocha --recursive -c -w",
    "bundle-client": "webpack --config webpack.client.js --release",
    "bundle-server": "webpack --config webpack.server.js --release",
    "bundle": "rm -rf dist && mkdir dist && cp -rf public dist/public && npm run bundle-client && npm run bundle-server",
    "start": "node dist/server.js",
    "watch-client": "webpack-dev-server --config webpack.client.js --content-base \"http://localhost:8080\"  --inline --hot --host localhost --port 8888",
    "watch-server": "nodemon -w 'client/server.js' -w 'client/views' -w 'webpack.babel.js' -x 'webpack --config webpack.server.js && node dist/server.js'",
    "dev": "concurrent --kill-others \"npm run watch-server\" \"npm run watch-client\""
  },
  "dependencies": {
    "alt": "^0.17.1",
    "babel": "^5.8.21",
    "body-parser": "^1.13.3",
    "classnames": "^2.1.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "purecss": "^0.6.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "superagent": "^1.3.0"
  },
  "babel": {
    "env": {
      "production": {
        "optional": [
          "optimisation",
          "minification"
        ]
      },
      "development": {
        "loose": "all"
      }
    }
  }
}
