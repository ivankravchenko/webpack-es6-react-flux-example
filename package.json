{
  "name": "myacc-react",
  "version": "0.0.1",
  "main": "server/main.js",
  "devDependencies": {
    "babel-core": "^5.8.8",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "concurrently": "^0.1.1",
    "css-loader": "^0.15.6",
    "eslint": "^1.0.0",
    "eslint-plugin-react": "^3.1.0",
    "jsdom": "^3.1.2",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.2.5",
    "mockery": "^1.4.0",
    "path": "^0.11.14",
    "react-hot-loader": "^1.2.8",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.5",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.10.1"
  },
  "scripts": {
    "start": "NODE_ENV=production node --harmony server/main.js",
    "build": "NODE_ENV=production webpack -p --progress --colors",
    "lint": "./node_modules/.bin/eslint . --ext .js,.jsx; exit 0",
    "test": "./node_modules/.bin/mocha --recursive -c",
    "watch-test": "./node_modules/.bin/mocha --recursive -c -w",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "watch-client": "NODE_ENV=development node server/webpack-dev-server.js",
    "start-dev": "NODE_ENV=development node server/main.js"
  },
  "dependencies": {
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "alt": "^0.17.1",
    "babel": "^5.8.12",
    "classnames": "^2.1.3",
    "express": "^4.13.1",
    "normalize.css": "^3.0.3"
  },
  "babel": {
    "env": {
      "production": {
        "optional": ["optimisation", "minification"]
      },
      "development": {
        "loose": "all"
      }
    }
  }
}
